#include <iostream>
using namespace std;
int main()
{
int a,b,c;
cout << "Enter three numbers: ";
cin >> a >> b >> c;
int mid;
if ((a >= b && a <= c) || (a <= b && a >= c))
{
mid = a;
}
else if ((b >= a && b <= c) || (b <= a && b >= c))
{
mid = b;
}
else
{
mid = c;
}
cout << "The middle number is: " << mid;}
